<?xml version="1.0" encoding="utf-8"?>
<manifest package="sound.presentation.basic.com.erez.assistantchat"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".misc.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login_ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".connection_ui.ConnectionActivity">
        </activity>
        <activity android:name=".chat_ui.ChatActivityLastMsg">

        </activity>
    </application>

</manifest>
